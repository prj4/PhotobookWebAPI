@model IEnumerable<PhotoBookDatabase.Model.Picture>
@{
    ViewData["Title"] = "Index";
}

<h1>
    Pictures
</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PictureId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EventPin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GuestId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HostId)
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PictureId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EventPin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GuestId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HostId)
            </td>
        </tr>
    }
    </tbody>
</table>

